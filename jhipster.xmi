<?xml version="1.0" encoding="ASCII"?>
<jdl:JdlDomainModel xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jdl="http://www.github.io/jhipster/jdl/JDL">
  <features xsi:type="jdl:JdlEntity" name="Region">
